<Window x:Class="Leaderboard.WinApp.View.SignInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Sign In" Height="190" Width="300">
    <Grid>
        <Label x:Name="label" Content="Username" HorizontalAlignment="Left" Margin="15,27,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.254,0.692" Width="74"/>
        <TextBox x:Name="usernameBox" HorizontalAlignment="Left" Height="23" Margin="94,27,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183"/>
        <Label x:Name="label1" Content="Password" HorizontalAlignment="Left" Margin="15,65,0,0" VerticalAlignment="Top" Width="71"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="94,66,0,0" VerticalAlignment="Top" Width="183" Height="25"/>
        <Button x:Name="signInButton" Content="Sign In" HorizontalAlignment="Left" Margin="101,113,0,0" VerticalAlignment="Top" Width="89"/>

    </Grid>
</Window>
